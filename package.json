  "dependencies": {
    "chalk": "^4.1.2",
    "discord.js": "^14.8.0",
    "mongoose": "^7.0.2",
    "validate-color": "^2.2.4"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
