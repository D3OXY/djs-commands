{
  "name": "@d3oxy/djs-commands",
  "repository": {
    "type": "git",
    "url": "https://github.com/D3OXY/djs-commands"
  },
  "version": "1.2.2",
  "main": "./dist/index.js",
  "typings": "./typings.d.ts",
  "files": [
    "dist",
    "typings.d.ts",
    "README.md"
  ],
  "description": "A feature rich command handler for discord.js",
  "scripts": {
    "build": "tsc --outDir dist --rootDir src --skipLibCheck",
